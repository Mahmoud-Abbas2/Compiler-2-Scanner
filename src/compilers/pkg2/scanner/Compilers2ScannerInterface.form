        }
        try {
            csa.Scanner(jTextArea1, jTable1, "Files/code.txt", jLabel8);
        } catch (IOException ex) {
            Logger.getLogger(Compilers2ScannerInterface.class.getName()).log(Level.SEVERE, null, ex);
        }
        
        File file = new File("Files/linesGenerator.txt");
        FileWriter fw; 
        
        try {
            fw = new FileWriter(file);
            fw.write(jTextArea1.getText());
            fw.close();
             LineNumberReader r = new LineNumberReader(new FileReader("Files/linesGenerator.txt"));
        
            String Line="";

            while ((Line = r.readLine()) != null) {
                if(compilers.pkg2.scanner.Compilers2ScannerAlgorithm.errorLines.contains(r.getLineNumber())){

                jTextArea2.append(String.valueOf(r.getLineNumber()+" (e)"+"\n"));

                }else{
                jTextArea2.append(String.valueOf(r.getLineNumber()+"\n"));

                }
            }
            compilers.pkg2.scanner.Compilers2ScannerAlgorithm.errorLines.clear();
        } catch (IOException ex) {
            Logger.getLogger(Compilers2ScannerInterface.class.getName()).log(Level.SEVERE, null, ex);
        }
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       if(comment == 0){
        comment = 1;//comment mode active
        
       try{
            FileWriter rw = new FileWriter("Files/Comment.txt");
            rw.write("/#"+jTextArea1.getText()+"#/");//prepare comment format
            rw.close();

            LineNumberReader r = new LineNumberReader(new FileReader("Files/Comment.txt"));
            String Line="";
            //applying comment
            while ((Line = r.readLine()) != null) {
                 if(r.getLineNumber() == 1)
                     jTextArea1.setText(Line+"\n");
                 else
                     jTextArea1.append(Line+"\n");
            }
        }catch(Exception e){
           System.out.println("");
       }
       }
    }                                        

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        //checking if the comment botton is clicked
        if(comment == 1){
            String uncomment = jTextArea1.getText();
            StringBuffer sb= new StringBuffer(uncomment);
            if(sb.charAt(0) == '/' && sb.charAt(1) == '#' && sb.charAt(sb.length()-3) == '#' && sb.charAt(sb.length()-2) == '/'){
            //removing comments marks 
            sb.deleteCharAt(0);
            sb.deleteCharAt(0);
            sb.deleteCharAt(sb.length()-2);
            sb.deleteCharAt(sb.length()-2);
            }
            
            jTextArea1.setText(String.valueOf(sb));
            comment = 0;//deactivate comment mode
        }
    }                                        

    private void jTextArea1KeyReleased(java.awt.event.KeyEvent evt) {                                       
        
        if(evt.getKeyCode() == KeyEvent.VK_BACK_SPACE || evt.getKeyCode()==KeyEvent.VK_DELETE){
        //to let me delete ab back after autocomplete    
        }
        else{
            String allCharacters = jTextArea1.getText();
            int currentLetterPosition=jTextArea1.getSelectionEnd();
            try{
            char currentLetter = allCharacters.charAt(currentLetterPosition-1);
            if(currentLetter!=' '&&currentLetter!='\n'){
                _1st4letters = _1st4letters + currentLetter;
                if(_1st4letters.length() == 4){
                    
                    for(int j = 0;j<12;j++){
                        for(int k = 0;k<4;k++){
                            _1st4letterReserved = _1st4letterReserved+reservedWords[j].charAt(k);
                            
                           
                    
                        }
                        if(_1st4letterReserved.equals(_1st4letters)){
                                int start = jTextArea1.getSelectionEnd()-3;
                                int end = jTextArea1.getSelectionEnd();
                                String Reserved = "";
                                for(int n = 1;n<=reservedWords[j].length()-1;n++){
                                    Reserved = Reserved + reservedWords[j].charAt(n);
                                }
                                jTextArea1.replaceRange(Reserved,start,end);
                                break;
                        }_1st4letterReserved="";
                        
                        
                    }
                    _1st4letters = "";
                    _1st4letterReserved="";
                }
            }else if (currentLetter=='\n' || currentLetter==' '){
                _1st4letters = "";
                _1st4letterReserved="";
            }
            }catch(Exception e){System.out.println("");}
            
        }
        
    }                                      

